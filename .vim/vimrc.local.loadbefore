" To add more plugins (list is in vim-plug style):
    if !exists("added_plugins")
        let added_plugins = []
    endif
    let added_plugins = added_plugins + [      
        \ ['leafgarland/typescript-vim', {}],
        \ ['peitalin/vim-jsx-typescript', {}],
    \]

" To add more coc.nvim default installed plugins:
